#include <iostream>
#include <cmath>
using namespace std;
int main() {
	cout << round(2.4) << endl;
	cout << round(3.6) << endl;
	cout << round(6.6) << endl;
	cout << sqrt(50) << endl;
	cout << round(sqrt(50)) << endl;
	cout << round(7.7) / round(8.8) << endl;
	cout << round(6.8) + round(10.9) << endl;
	cout << sqrt(70) << endl;
	cout << round(sqrt(70)) << endl;
	cout << round(56.8) + 4 << endl;
	int Base=9, Power=2;
	cout << pow(Base, Power) << endl;
	cout << pow(10, 3) << endl;
	cout << pow(4, 4) << endl;
	int x = 2;
	cout << sqrt(x) << endl;
	cout << pow(2, 5) << endl;
	cout << pow(x, 3) << endl;
	cout << ceil(2.9) << endl;
	cout << floor(2.9) << endl;
	cout << ceil(-2.9) << endl;
	cout << floor(-2.9) << endl;
	cout << ceil(2.1) << endl;
	cout << round(2.1) << endl;
	cout << floor(2.1) << endl;
	cout << abs(10) << endl;
	cout << abs(-10) << endl;
	cout << abs(-23) << endl;
	cout << abs(-45) << endl;
	return 0;
}

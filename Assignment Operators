#include <iostream>
using namespace std;
int main() {
	int A = 10, B = 20;
	cout << "A = " << A << endl;
	A += B;// A = A + B
	cout << "A = " << A << endl;
	A -= B; // A = A - B
	cout << "A = " << A << endl;
	A *= B; // A = A * B
	cout << "A = " << A << endl;
	A /= B; // A = A / B
	cout << "A = " << A << endl;
	A %= B; // A = A % B
	cout << "A = " << A << endl;
	return 0;
}
